typedef union
{
  int int_;
  char char_;
  double double_;
  int string_;
  ZapZloz zapzloz_;
  Zapytanie zapytanie_;
  LiniaZapytania liniazapytania_;
  Wyraz wyraz_;
  ListZapytanie listzapytanie_;
  ListLiniaZapytania listliniazapytania_;
  int przerwa_;
  ListPrzerwa listprzerwa_;
  Tekst tekst_;
  Nazwa nazwa_;
} YYSTYPE;

#define _ERROR_ 258
